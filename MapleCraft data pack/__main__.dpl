				print("}")
				return
			
			if start == end:
				return
			
			mid = (start + end) // 2
			mid_right = mid + 1
			
			if start == 0 and end == len(cmd_list) - 1:
				print("func run()")
			else:
				print(f"func {start}_{end}()")
			print("{")
